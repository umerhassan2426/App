<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Result">
    <TextView
        android:id="@+id/resultview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="38dp"
        android:layout_marginTop="69dp"
        android:text="Total Questions" />

    <TextView
        android:id="@+id/resultview2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/textView"
        android:layout_marginTop="180dp"
        android:text="Correct Answer" />

    <TextView
        android:id="@+id/resultview3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/textView"
        android:layout_marginBottom="186dp"
        android:text="Wrong Answer" />

    <TextView
        android:id="@+id/resultview4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/textView"
        android:layout_marginEnd="75dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/resultview5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/resultview4"
        android:layout_alignTop="@+id/resultview2"
        android:text="TextView" />

    <TextView
        android:id="@+id/resultview6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/resultview4"
        android:layout_alignTop="@+id/resultview3"
        android:text="TextView" />

</RelativeLayout>